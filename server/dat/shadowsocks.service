[Unit]
Description=ShadowSocks - An socks5 proxy
Wants=network-online.target
After=network-online.target
Before=kcptun.service

[Service]
Type=simple
PIDFile=/run/shadowsocks.pid
ExecStart=/usr/local/bin/ssserver -c /etc/ics-gfw/config.json -d start
ExecStop=/usr/local/bin/ssserver -d stop
Restart=/usr/local/bin/ssserver -c /etc/ics-gfw/config.json -d restart

[Install]
WantedBy=multi-user.target kcptun.service
